local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library/main/Library", true))()

local window = library:AddWindow("° TANJIN|°", {main_color = Color3.fromRGB(255, 0, 0), min_size = Vector2.new(450, 350), toggle_key = Enum.KeyCode.LeftControl})

-- Create a New Tab
local newTab = window:AddTab("main")


 newTab:AddButton("ANTI AFK", function() loadstring(game:HttpGet('https://pastebin.com/raw/TRCYuX46'))()
    print("loadstring(game:HttpGet('https://pastebin.com/raw/TRCYuX46'))()")
end)


local features = window:AddTab("fast rebirth√") features:Show()

features:AddLabel("TANJIN")

autoRebirth = false

local switch = features:AddSwitch("Auto Rebirth", function(state) autoRebirth = state if state then task.spawn(function() while autoRebirth do c = game:GetService("Players").LocalPlayer a = game:GetService("ReplicatedStorage")

function unequipAllPets() f = c.petsFolder for _, h in pairs(f:GetChildren()) do if h:IsA("Folder") then for _, j in pairs(h:GetChildren()) do a.rEvents.equipPetEvent:FireServer("unequipPet", j) end end end task.wait(.1) end

function equipPet(petName)
            unequipAllPets()
            task.wait(.01)
            for _, n in pairs(c.petsFolder.Unique:GetChildren()) do
                if n.Name == petName then
                    a.rEvents.equipPetEvent:FireServer("equipPet", n)
                end
            end
        end

        function getMachine(machineName)
            machine = workspace.machinesFolder:FindFirstChild(machineName)
            if not machine then
                for _, s in pairs(workspace:GetChildren()) do
                    if s:IsA("Folder") and s.Name:find("machines") then
                        machine = s:FindFirstChild(machineName)
                        if machine then break end
                    end
                end
            end
            return machine
        end

        function interact()
            u = game:GetService("VirtualInputManager")
            u:SendKeyEvent(true, "E", false, game)
            task.wait(.1)
            u:SendKeyEvent(false, "E", false, game)
        end

        v = c.leaderstats.Rebirths.Value
        w = 10000 + (5000 * v)

        if c.ultimatesFolder:FindFirstChild("Golden Rebirth") then
            x = c.ultimatesFolder["Golden Rebirth"].Value
            w = math.floor(w * (1 - (x * 0.1)))
        end

        unequipAllPets()
        task.wait(.1)
        equipPet("Swift Samurai")

        while c.leaderstats.Strength.Value < w do
            for _ = 1, 10 do
                c.muscleEvent:FireServer("rep")
            end
            task.wait()
        end

        unequipAllPets()
        task.wait(.1)
        equipPet("Tribal Overlord")

        z = getMachine("Jungle Bar Lift")
        if z and z:FindFirstChild("interactSeat") then
            c.Character.HumanoidRootPart.CFrame = z.interactSeat.CFrame * CFrame.new(0, 3, 0)
            repeat task.wait(.1) interact() until c.Character.Humanoid.Sit
        end

        A = c.leaderstats.Rebirths.Value
        repeat
            a.rEvents.rebirthRemote:InvokeServer("rebirthRequest")
            task.wait(.1)
        until c.leaderstats.Rebirths.Value > A

        task.wait()
    end
end)

end

end)

switch:Set(false)


-- Create a New Tab
local newTab

local features = window:AddTab("fast grind")

features:AddSwitch("Fast grind", function(state)
    _G.SmartGrind = state
    
    if state then
        task.spawn(function()
            local player = game:GetService("Players").LocalPlayer
            local muscleEvent = player.muscleEvent
            
            -- Pet setup (optimized one-time call)
            local swiftSamurais = {}
            for _, pet in pairs(player.petsFolder.Unique:GetChildren()) do
                if pet.Name == "Swift Samurai" then
                    table.insert(swiftSamurais, pet)
                end
            end
            game:GetService("ReplicatedStorage").rEvents.equipPetEvent:FireServer("equipMultiPet", swiftSamurais)

            -- 3-Stage Burst System
            while _G.SmartGrind do
                -- Stage 1: Fast Start (100 reps)
                for i = 1, 100 do
                    muscleEvent:FireServer("rep")
                end
                task.wait(0.01)
                
                -- Stage 2: Mid Boost ( 30 reps)
                for i = 1, 50 do
                    muscleEvent:FireServer("rep")
                end
                task.wait(0.01)
                
                -- Stage 3: Slow Finish (10 reps)
                for i = 1, 10 do
                    muscleEvent:FireServer("rep")
                end
                task.wait(0.01)
                
                -- Randomize pattern occasionally
                if math.random(1, 20) == 1 then
                    task.wait(math.random(0.1, 0.3))
                end
            end
            
            -- Cleanup
            game:GetService("ReplicatedStorage").rEvents.equipPetEvent:FireServer("unequipAllPets")
        end)
    end
end)
